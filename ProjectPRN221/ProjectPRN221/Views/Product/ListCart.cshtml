	<section id="cart_items">
		<div class="container">
			<div class="breadcrumbs">
				<ol class="breadcrumb">
					<li><a href="#">Home</a></li>
					<li class="active">Check out</li>
				</ol>
			</div>

			<div class="shopper-informations">
				@if (ViewBag.cart != null)
				{
					<div class="panel-body" id="noti-box">
						<div class="alert alert-block alert-danger">
							<button data-dismiss="alert" class="close close-sm" type="button">
								<i class="fa fa-times"></i>
							</button>
							<strong>Oh snap!</strong> @ViewBag.cart
						</div>
					</div>
				}
				<div class="row">
					<div class="col-sm-3">
						<div class="shopper-info">
							<p>Shopper Information</p>
							<form asp-action="Order" asp-controller="Product" method="post">
								<input type="text" placeholder="Display Name" value="@ViewBag.acc.AccountUsername">
                                <input type="text" placeholder="User Name" value="@ViewBag.acc.AccountName">
								<input type="hidden" name="Id" value="@ViewBag.lst.CartId"/>
								<a class="btn btn-primary" href="">Order</a>
							</form>	
						</div>
					</div>
				</div>
			</div>
			<div class="review-payment">
				<h2>Review & Payment</h2>
			</div>

			<div class="table-responsive cart_info">
				<table class="table table-condensed">
					<thead>
						<tr class="cart_menu">
							<td class="image">Item</td>
							<td class="description"></td>
							<td class="price">Price</td>
							<td class="quantity">Quantity</td>
							<td class="total">Total</td>
							<td></td>
						</tr>
					</thead>
					<tbody>

                        @if (ViewBag.lst.AccountId == ViewBag.acc.AccountId)
                        {
                            @foreach (var item in ViewBag.lstCart)
                            {
                                @foreach (var i in ViewBag.lstProduct)
                                {
                                    @if (item.ProductId == i.ProductId)
                                    {
                                        <tr>
                                            <td class="cart_product">
                                                <a href=""><img class="media-object" src="~/images/product-details/product/@i.ProductImage" alt=""></a>
                                            </td>
                                            <td class="cart_description">
                                                <h4><a href="">@i.ProductName</a></h4>
                                                <p>Web ID: @i.ProductCode</p>
                                            </td>
                                            <td class="cart_price">
                                                <p>$@i.ProductPrice</p>
                                            </td>
                                            <td class="cart_quantity">
                                                <div class="cart_quantity_button">
                                                    @*<a class="cart_quantity_up" href="/Product/Up?Id="> + </a>*@
                                                    <input type="number" class="cart_quantity_input" style="width: 35px" name="quantity" value="1" autocomplete="off" size="2">
                                                    @*<a class="cart_quantity_down" href="/Product/Down?Id="> - </a>*@
                                                </div>
                                            </td>
                                            <td class="cart_total">
												<p class="cart_total_price">$@(i.ProductPrice * item.Quantity)</p>
                                            </td>
                                            <td class="cart_delete">
												<a class="cart_quantity_delete" href="/Product/DeleteCart?PId=@i.ProductId&CId=@ViewBag.lst.CartId"><i class="fa fa-times"></i></a>
                                            </td>
                                        </tr>
                                    }
                                }
                            }
                        }		
					</tbody>
				</table>
			</div>
		</div>
	</section> 



